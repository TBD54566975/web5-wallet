{
  "name": "web5wallet",
  "version": "0.0.2",
  "buildNumber": "20",
  "main": "index.js",
  "packageManager": "yarn@4.2.2+sha256.1aa43a5304405be7a7cb9cb5de7b97de9c4e8ddd3273e4dad00d6ae3eb39f0ef",
  "scripts": {
    "start": "watchman watch-del-all; rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/react-native-packager-cache-*; rm -rf $TMPDIR/metro-cache; rm -rf $TMPDIR/haste-map-react-native-packager-*;expo start --clear",
    "android:release": "cd android && ./gradlew :app:bundleRelease && open app/build/outputs/bundle/release/;cd ..",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "pods": "cd ios; bundle exec pod install;cd ..",
    "clean": "rm -rf node_modules/; yarn cache clean; yarn install; watchman watch-del-all; rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/react-native-packager-cache-*; rm -rf $TMPDIR/metro-cache; rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "clean:android": "cd android;./gradlew clean; ./gradlew --stop; cd ..",
    "clean:ios": "rm -rf ios/build; pod cache clean --all; rm -rf ios/Pods; cd ios; xcodebuild -alltargets clean; cd ..; yarn run pods",
    "clean:platforms": "yarn run clean; yarn run clean:android; yarn run clean:ios",
    "clean:deep": "rm -rf ~/.cocoapods; rm -rf /Library/Developer/Xcode/DerivedData; rm -rf ~/.gradle/; rm -rf $(getconf DARWIN_USER_CACHE_DIR)/org.llvm.clang/ModuleCache; rm -rf $(getconf DARWIN_USER_CACHE_DIR)/org.llvm.clang.$(whoami)/ModuleCache; rm -fr ~/Library/Caches/com.apple.dt.Xcode/; yarn run clean:platforms",
    "postinstall": "patch-package",
    "deploy:android:development:release": "cd android && bundle exec fastlane deploy --env development.release; cd ..",
    "deploy:android:production:release": "cd android && bundle exec fastlane deploy --env production.release; cd ..",
    "deploy:ios:development:release": "cd ios && bundle exec fastlane deploy --env development.release; cd ..",
    "deploy:ios:production:release": "cd ios && bundle exec fastlane deploy --env production.release; cd ..",
    "symlink:env:fastlane": "node ./scripts/symlink-env-fastlane.js",
    "link:web5": "yarn add @web5/agent@link:../web5-js/packages/agent/ @web5/api@link:../web5-js/packages/api/ @web5/common@link:../web5-js/packages/common/ @web5/credentials@link:../web5-js/packages/credentials @web5/crypto@link:../web5-js/packages/crypto @web5/dids@link:../web5-js/packages/dids/ @web5/identity-agent@link:../web5-js/packages/identity-agent",
    "unlink:web5": "yarn add @web5/agent @web5/api @web5/common @web5/credentials @web5/crypto @web5/dids @web5/identity-agent"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "6.0.5",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@noble/ciphers": "^0.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@shamilovtim/react-native-leveldb-async": "^1.0.0",
    "@tanstack/react-query": "^5.51.23",
    "@tbd54566975/dwn-sdk-js": "^0.4.5",
    "@tbd54566975/web5-react-native-metro-config": "^1.0.3",
    "@tbd54566975/web5-react-native-polyfills": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@web5/agent": "link:../web5-js/packages/agent",
    "@web5/api": "link:../web5-js/packages/api",
    "@web5/common": "link:../web5-js/packages/common",
    "@web5/credentials": "link:../web5-js/packages/credentials",
    "@web5/crypto": "link:../web5-js/packages/crypto",
    "@web5/dids": "link:../web5-js/packages/dids",
    "@web5/identity-agent": "link:../web5-js/packages/identity-agent",
    "expo": "51.0.28",
    "expo-camera": "^15.0.14",
    "expo-local-authentication": "~14.0.1",
    "expo-secure-store": "^13.0.2",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "~1.12.1",
    "query-string": "^7.1.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-blob-jsi-helper": "0.3.1",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-keyboard-controller": "1.13.1",
    "react-native-leveldb": "3.3.5",
    "react-native-quick-crypto": "0.7.3",
    "react-native-reanimated": "^3.15.0",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-webview": "^13.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.36",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-etc": "2.0.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "4.1.0",
    "patch-package": "8.0.0",
    "postinstall-postinstall": "2.1.0",
    "typescript": "^5.5.4"
  },
  "expo": {
    "install": {
      "exclude": [
        "expo",
        "expo-camera",
        "react-native-gesture-handler",
        "react-native-reanimated",
        "react-native-safe-area-context",
        "react-native-screens",
        "react-native-webview",
        "@types/react",
        "typescript"
      ]
    }
  },
  "private": true
}
