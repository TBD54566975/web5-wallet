{
  "title": "School",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "address": { "$ref": "Address" },
    "concentration": { "type": "string" },
    "graduated": { "type": "boolean" },
    "dateStarted": { "type": "string", "format": "date" },
    "dateEnded": { "type": "string", "format": "date" },
    "degree": { "type": "string" }
  }
}
